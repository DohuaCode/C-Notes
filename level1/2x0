#include <stdio.h>
#include <stdlib.h>
char *ft_ctrcpy(char *dest, char *src);

int main()
{
char *src = "ass";   
char *dest = malloc(sizeof(src)+1);
ft_ctrcpy(dest, src);
//check
printf("copied string %s , address of copied string %p\n",dest,dest);
printf("source string %s , address of copied string %p\n",src,src);

}

char *ft_ctrcpy(char *dest, char *src)

{
for(int i = 0;i <sizeof(src)+1;i++){
   dest[i] = src[i]; 
}
//check
printf(" fromfunc copied string %s , address of copied string %p\n",dest,dest);
printf("fromfunc source string %s , address of copied string %p\n",src,src);

}
